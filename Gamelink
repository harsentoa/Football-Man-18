const countryNameMap = { "Russia": "러시아", "Saudi Arabia": "사우디", "Egypt": "이집트", "Uruguay": "우루과이", "Portugal": "포르투갈", "Spain": "스페인", "Morocco": "모로코", "Iran": "이란", "France": "프랑스", "Australia": "호주", "Peru": "페루", "Denmark": "덴마크", "Argentina": "아르헨티나", "Iceland": "아이슬란드", "Croatia": "크로아티아", "Nigeria": "나이지리아", "Brazil": "브라질", "Switzerland": "스위스", "Costa Rica": "코스타리카", "Serbia": "세르비아", "Germany": "독일", "Mexico": "멕시코", "Sweden": "스웨덴", "South Korea": "대한민국", "Belgium": "벨기에", "Panama": "파나마", "Tunisia": "튀니지", "England": "잉글랜드", "Poland": "폴란드", "Senegal": "세네갈", "Colombia": "콜롬비아", "Japan": "일본" };

let gameState = 'TEAM_SELECTION';
let previousGameState = '';
let userTeam = null;
let teams = [];
let groups = [];
let tournamentBracket = {};
let buttons = {};
let NotoSansKR;
let currentMatch = null;
let currentMatchResult = null;
let currentStage = '';
let matchLog = [];
let gameMinute = 0;
let awards = {};
let userSquad = { lineup: [], substitutes: [] };
let squadScrollOffset = 0;
let logScrollOffset = 0;
let selectedPlayer = null;

const framesPerGameMinute = 30;
let frameCounterForSim = 0;

function setup() {
    createCanvas(1280, 720);
    textFont(NotoSansKR || 'sans-serif');
    resetGame();
}

function resetGame() {
    gameState = 'TEAM_SELECTION';
    userTeam = null;
    groups = [];
    tournamentBracket = {};
    awards = {};
    currentStage = '';
    currentMatch = null;
    userSquad = { lineup: [], substitutes: [] };
    squadScrollOffset = 0;
    logScrollOffset = 0;
    selectedPlayer = null;
    populatePlayerStats();
    teams = Object.keys(countryNameMap);
    setupButtons();
}

function draw() {
    background(20, 40, 60);
    switch (gameState) {
        case 'TEAM_SELECTION':
            drawTeamSelectionScreen();
            break;
        case 'GROUP_DRAW':
            drawGroupDrawScreen();
            break;
        case 'GROUP_STAGE':
            drawGroupStageScreen();
            break;
        case 'PRE_MATCH_BRIEFING':
            drawPreMatchBriefingScreen();
            break;
        case 'SQUAD_MANAGEMENT':
            drawSquadManagementScreen();
            break;
        case 'MATCH_SIMULATION':
            updateMatchSimulation();
            drawMatchSimulationScreen();
            break;
        case 'MATCH_RESULT':
            drawMatchResultScreen();
            break;
        case 'KNOCKOUT':
            drawKnockoutScreen();
            break;
        case 'AWARDS_CEREMONY':
            drawAwardsScreen();
            break;
        case 'ROSTER_VIEW':
            drawRosterScreen();
            break;
        case 'PLAYER_DETAIL':
            drawPlayerDetailScreen();
            break;
    }
}

function mousePressed() {
    const activeButtons = buttons[gameState] || [];
    for (const btn of activeButtons) {
        if (mouseX > btn.x && mouseX < btn.x + btn.w && mouseY > btn.y && mouseY < btn.y + btn.h) {
            btn.onClick();
            return;
        }
    }
    if (gameState === 'SQUAD_MANAGEMENT') {
        handleSquadManagementClick();
    } else if (['PRE_MATCH_BRIEFING', 'MATCH_RESULT', 'ROSTER_VIEW'].includes(gameState)) {
        handlePlayerNameClick();
    }
}


function mouseWheel(event) {
    if (gameState === 'SQUAD_MANAGEMENT') {
        squadScrollOffset += event.delta;
        const listHeight = max(userSquad.lineup.length, userSquad.substitutes.length) * 30;
        const viewHeight = height - 180;
        const maxScroll = -max(0, listHeight - viewHeight);
        squadScrollOffset = constrain(squadScrollOffset, maxScroll, 0);
        return false;
    }
    if (gameState === 'MATCH_SIMULATION') {
        logScrollOffset += event.delta;
        const logHeight = matchLog.length * 25;
        const viewHeight = height - 150;
        const maxScroll = -max(0, logHeight - viewHeight);
        logScrollOffset = constrain(logScrollOffset, maxScroll, 0);
        return false;
    }
}

function setupButtons() {
    buttons.TEAM_SELECTION = [];
    const btnW = 180,
        btnH = 35,
        gap = 10,
        cols = 4;
    teams.forEach((teamName, i) => {
        const col = i % cols,
            row = Math.floor(i / cols);
        buttons.TEAM_SELECTION.push({
            x: 20 + col * (btnW + gap),
            y: 100 + row * (btnH + gap),
            w: btnW,
            h: btnH,
            text: countryNameMap[teamName] || teamName,
            onClick: () => {
                userTeam = teamName;
                gameState = 'GROUP_DRAW';
            }
        });
    });
    buttons.GROUP_DRAW = [{
        x: width / 2 - 210,
        y: 200,
        w: 200,
        h: 50,
        text: '실제 조 편성',
        onClick: () => setupGroups('actual')
    }, {
        x: width / 2 + 10,
        y: 200,
        w: 200,
        h: 50,
        text: '랜덤 조 추첨',
        onClick: () => setupGroups('random')
    }];

    const rosterViewAction = () => {
        previousGameState = gameState;
        gameState = 'ROSTER_VIEW';
    };
    buttons.GROUP_STAGE = [{
        x: width - 200,
        y: height - 110,
        w: 180,
        h: 40,
        text: '선수단 보기 🧑‍🤝‍🧑',
        onClick: rosterViewAction
    }, {
        x: width - 200,
        y: height - 60,
        w: 180,
        h: 40,
        text: '다음 경기 진행 ⏩',
        onClick: () => advanceTournament()
    }];
    buttons.PRE_MATCH_BRIEFING = [{
        x: width / 2 - 220,
        y: height - 80,
        w: 200,
        h: 50,
        text: '스쿼드 변경',
        onClick: () => {
            squadScrollOffset = 0;
            gameState = 'SQUAD_MANAGEMENT';
        }
    }, {
        x: width / 2 + 20,
        y: height - 80,
        w: 200,
        h: 50,
        text: '킥오프! ⚽',
        onClick: () => startMatchSimulation()
    }];
    buttons.MATCH_RESULT = [{
        x: width / 2 - 100,
        y: height - 60,
        w: 200,
        h: 40,
        text: '다음으로 ➡️',
        onClick: () => returnToTournamentView()
    }];
    buttons.KNOCKOUT = [{
        x: width - 200,
        y: height - 110,
        w: 180,
        h: 40,
        text: '선수단 보기 🧑‍🤝‍🧑',
        onClick: rosterViewAction
    }, {
        x: width - 200,
        y: height - 60,
        w: 180,
        h: 40,
        text: '다음 일정 진행 ⏩',
        onClick: () => advanceTournament()
    }];
    buttons.AWARDS_CEREMONY = [{
        x: width / 2 - 125,
        y: height - 70,
        w: 250,
        h: 50,
        text: '새 게임 시작 🔄',
        onClick: () => resetGame()
    }];
    buttons.SQUAD_MANAGEMENT = [{
        x: width / 2 - 100,
        y: height - 60,
        w: 200,
        h: 40,
        text: '스쿼드 확정',
        onClick: () => {
            const {
                isValid,
                message
            } = validateSquad(userSquad);
            if (isValid) {
                gameState = 'PRE_MATCH_BRIEFING';
            } else {
                alert(message);
            }
        }
    }];
    
    // [수정됨] ROSTER_VIEW의 버튼을 '대진표 보기'와 '경기 시작하기'로 변경
    buttons.ROSTER_VIEW = [
        {
            x: width / 2 - 220,
            y: height - 60,
            w: 200,
            h: 40,
            text: '대진표 보기 📊',
            onClick: () => {
                gameState = previousGameState;
            }
        },
        {
            x: width / 2 + 20,
            y: height - 60,
            w: 200,
            h: 40,
            text: '경기 시작하기 ⚽',
            onClick: () => advanceTournament()
        }
    ];

    buttons.PLAYER_DETAIL = [{
        x: width / 2 - 100,
        y: height - 60,
        w: 200,
        h: 40,
        text: '돌아가기 ↩️',
        onClick: () => {
            gameState = previousGameState;
            selectedPlayer = null; // 돌아갈 때 선택된 선수 정보 초기화
        }
    }];
}


function drawButton(btn) {
    const isHover = mouseX > btn.x && mouseX < btn.x + btn.w && mouseY > btn.y && mouseY < btn.y + btn.h;
    fill(isHover ? color(100, 200, 255) : color(220));
    stroke(10);
    rect(btn.x, btn.y, btn.w, btn.h, 8);
    fill(isHover ? 0 : 30);
    noStroke();
    textSize(btn.h > 40 ? 20 : 16);
    textAlign(CENTER, CENTER);
    text(btn.text, btn.x + btn.w / 2, btn.y + btn.h / 2);
}

// =================================================
//   화면 그리기 함수들
// =================================================
function drawTeamSelectionScreen() {
    fill(255);
    textSize(32);
    textAlign(CENTER, CENTER);
    text("⚽ 2018 월드컵 매니저 ⚽", width / 2, 50);
    buttons.TEAM_SELECTION.forEach(btn => drawButton(btn));
}

function drawGroupDrawScreen() {
    fill(255);
    textSize(32);
    textAlign(CENTER, CENTER);
    text("조 추첨 방식을 선택하세요", width / 2, 100);
    textSize(18);
    text(`선택한 팀: ${countryNameMap[userTeam]}`, width / 2, 150);
    buttons.GROUP_DRAW.forEach(btn => drawButton(btn));
}

function drawGroupStageScreen() {
    textSize(28);
    textAlign(CENTER, CENTER);
    fill(255);
    text("🏆 조별 리그 순위 🏆", width / 2, 30);
    drawGroupTables();
    buttons.GROUP_STAGE.forEach(btn => drawButton(btn));
}

function drawPreMatchBriefingScreen() {
    fill(255);
    textSize(28);
    textAlign(CENTER, CENTER);
    text("📋 경기 전 브리핑 📋", width / 2, 40);
    const [teamA, teamB] = currentMatch;
    const squadA = getSquadByFormation(teamA.name);
    const squadB = getSquadByFormation(teamB.name);

    textSize(20);
    text(countryNameMap[teamA.name], width / 4, 80);
    text(countryNameMap[teamB.name], width * 3 / 4, 80);
    drawPlayerList(squadA, width / 4, 110, false);
    drawPlayerList(squadB, width * 3 / 4, 110, false);
    buttons.PRE_MATCH_BRIEFING.forEach(btn => drawButton(btn));
}

function drawPlayerList(squad, x, y, showRating = false) {
    const {
        lineup,
        substitutes
    } = squad;
    const align = (x < width / 2) ? RIGHT : LEFT;
    textSize(14);
    for (let i = 0; i < lineup.length; i++) {
        const player = lineup[i];
        const lineY = y + i * 24;
        let txt = `${player.koreanName} (${player.position})`;
        textAlign(align, TOP);
        if (showRating) {
            fill(255);
            if (currentMatchResult.scorers.some(s => s.player.name === player.name)) txt = '⚽ ' + txt;
            if (currentMatchResult.yellows.some(p => p.name === player.name)) txt = '🟨 ' + txt;
            if (currentMatchResult.reds.some(p => p.name === player.name)) txt = '🟥 ' + txt;
            text(txt, x + (align === RIGHT ? 60 : -60), lineY);
            textAlign(align === RIGHT ? LEFT : RIGHT, TOP);
            text(player.rating.toFixed(1), x + (align === RIGHT ? 70 : -70), lineY);
        } else {
            const c = player.condition;
            if (c > 95) fill(144, 238, 144);
            else if (c > 80) fill(255, 255, 224);
            else fill(255, 182, 193);
            text(txt, x + (align === RIGHT ? 60 : -60), lineY);
            textAlign(align === RIGHT ? LEFT : RIGHT, TOP);
            text(c > 95 ? '🟢' : (c > 80 ? '🟡' : '🔴'), x + (align === RIGHT ? 70 : -70), lineY);
        }
    }
    if (substitutes.length > 0) {
        const subStartY = y + lineup.length * 24 + 10;
        stroke(150);
        line(x - 100, subStartY, x + 100, subStartY);
        noStroke();
        textAlign(CENTER, TOP);
        fill(180);
        text("후보", x, subStartY + 5);
        for (let i = 0; i < substitutes.length; i++) {
            const p = substitutes[i];
            const lineY = subStartY + 25 + i * 20;
            fill(200, 180);
            text(`${p.koreanName} (${p.position})`, x, lineY);
        }
    }
}

function drawMatchSimulationScreen() {
    textAlign(CENTER, CENTER);
    fill(255);
    textSize(40);
    text(`${countryNameMap[currentMatch[0].name]} ${currentMatchResult.scoreA} : ${currentMatchResult.scoreB} ${countryNameMap[currentMatch[1].name]}`, width / 2, 50);
    textSize(24);
    fill(200);
    text(`${gameMinute}'`, width / 2, 100);

    const logYStart = 150;
    const logLineHeight = 25;
    textAlign(LEFT, TOP);
    textSize(16);
    fill(220);
    for (let i = 0; i < matchLog.length; i++) {
        const logY = logYStart + i * logLineHeight + logScrollOffset;
        if (logY > logYStart - logLineHeight && logY < height) {
            text(matchLog[i], 50, logY);
        }
    }
}

function drawMatchResultScreen() {
    textAlign(CENTER, CENTER);
    fill(255);
    textSize(28);
    text("📊 경기 결과 리포트 📊", width / 2, 30);
    const [teamA, teamB] = currentMatch;
    textSize(24);
    fill(255);
    text(countryNameMap[teamA.name], width / 4, 70);
    text(countryNameMap[teamB.name], width * 3 / 4, 70);
    textSize(36);
    text(`${currentMatchResult.scoreA} : ${currentMatchResult.scoreB}`, width / 2, 70);
    const drawScorers = (teamName, x) => {
        const scorers = currentMatchResult.scorers.filter(s => s.teamName === teamName);
        if (scorers.length > 0) {
            textAlign(CENTER, TOP);
            textSize(12);
            fill(200, 220, 255);
            let y = 95;
            scorers.forEach(s => {
                text(`⚽ ${s.player.koreanName} '${s.minute}`, x, y);
                y += 15;
            });
        }
    };
    drawScorers(teamA.name, width / 4);
    drawScorers(teamB.name, width * 3 / 4);
    const squadA = getSquadByFormation(teamA.name),
        squadB = getSquadByFormation(teamB.name);
    drawPlayerList(squadA, width / 4, 150, true);
    drawPlayerList(squadB, width * 3 / 4, 150, true);
    if (currentMatchResult.mom) {
        fill(255, 230, 150);
        textAlign(CENTER, TOP);
        textSize(16);
        text(`⭐ MoM: ${currentMatchResult.mom.koreanName} (${currentMatchResult.mom.rating.toFixed(1)})`, width / 2, height - 85);
    }
    buttons.MATCH_RESULT.forEach(btn => drawButton(btn));
}

function drawKnockoutScreen() {
    const stageName = {
        'round_of_16': '16강',
        'quarter_finals': '8강',
        'semi_finals': '준결승',
        'third_place_playoff': '3-4위전',
        'final': '결승'
    };
    textSize(28);
    textAlign(CENTER, CENTER);
    fill(255);
    text(`🏆 ${stageName[currentStage]} 대진표 🏆`, width / 2, 30);
    const bracket = tournamentBracket[currentStage];
    if (!bracket) return;
    const isFinals = ['final', 'third_place_playoff'].includes(currentStage);
    const matchHeight = isFinals ? height / 2 - 50 : 100;
    const maxMatches = isFinals ? 1 : (currentStage === 'semi_finals' ? 2 : 4);
    bracket.forEach((match, i) => {
        const xPos = isFinals ? width / 2 : (i < maxMatches ? width / 4 + 50 : width * 3 / 4 - 50);
        const yPos = 100 + (i % maxMatches) * matchHeight;
        const [team1, team2] = match.teams;
        if (!team1 || !team2) return;
        const team1Name = countryNameMap[team1.name],
            team2Name = countryNameMap[team2.name];
        textAlign(CENTER, CENTER);
        if (match.winner) {
            const isTeam1Winner = team1 && match.winner.name === team1.name;
            fill(isTeam1Winner ? 255 : 120);
            textSize(16);
            text(`${team1Name} ${match.scoreA}`, xPos, yPos - 15);
            fill(isTeam1Winner ? 120 : 255);
            text(`${team2Name} ${match.scoreB}`, xPos, yPos + 15);
        } else {
            fill(220);
            textSize(16);
            text(team1Name, xPos, yPos - 15);
            text("vs", xPos, yPos);
            text(team2Name, xPos, yPos + 15);
        }
    });
    buttons.KNOCKOUT.forEach(btn => drawButton(btn));
}

function drawAwardsScreen() {
    textAlign(CENTER, TOP);
    fill(255);
    textSize(32);
    text("🏆 월드컵 최종 시상식 🏆", width / 2, 40);
    const col1 = width / 4,
        col2 = width * 3 / 4,
        startY = 100;
    textSize(24);
    fill(255, 215, 0);
    text(`🥇 1위: ${countryNameMap[awards.first?.name]}`, col1, startY);
    fill(192, 192, 192);
    text(`🥈 2위: ${countryNameMap[awards.second?.name]}`, col1, startY + 50);
    fill(205, 127, 50);
    text(`🥉 3위: ${countryNameMap[awards.third?.name]}`, col1, startY + 100);
    const drawAward = (title, awardInfo, y) => {
        textSize(22);
        fill(255);
        text(title, col2, y);
        textSize(16);
        fill(220);
        let awardY = y + 35;
        awardInfo?.forEach(p => {
            text(`${p.name} (${countryNameMap[p.team]}) - ${p.stat}`, col2, awardY);
            awardY += 25;
        });
    };
    drawAward("⚽ 득점왕 (골든 부트)", awards.goldenBoot, startY);
    drawAward("👟 도움왕 (플레이메이커)", awards.assistKing, startY + 120);
    drawAward("✨ 신인상 (영 플레이어)", awards.youngPlayer, startY + 240);
    buttons.AWARDS_CEREMONY.forEach(btn => drawButton(btn));
}

function drawGroupTables() {
    const startX = 20,
        startY = 70,
        groupW = 370,
        groupH = 120,
        gap = 10;
    groups.forEach((group, index) => {
        const col = index % 2,
            row = Math.floor(index / 2);
        const x = startX + col * (groupW + gap),
            y = startY + row * (groupH + gap);
        fill(40, 60, 80, 200);
        stroke(150, 180);
        rect(x, y, groupW, groupH, 8);
        fill(255);
        noStroke();
        textSize(18);
        textAlign(LEFT, TOP);
        text(`Group ${group.name}`, x + 10, y + 8);
        textSize(12);
        const headers = ["#", "팀", "경기", "승", "무", "패", "득", "실", "차", "승점"];
        const colWidths = [20, 110, 25, 25, 25, 25, 25, 25, 30, 40];
        let currentX = x + 10;
        headers.forEach((header, i) => {
            fill(180);
            text(header, currentX, y + 35);
            currentX += colWidths[i];
        });
        stroke(100);
        line(x + 5, y + 52, x + groupW - 5, y + 52);
        noStroke();
        const sortedTeams = [...group.teams].sort((a, b) => b.points - a.points || b.gd - a.gd || b.gf - a.gf);
        sortedTeams.forEach((team, i) => {
            currentX = x + 10;
            const rowY = y + 60 + i * 15;
            const teamData = [i + 1, countryNameMap[team.name], team.wins + team.draws + team.losses, team.wins, team.draws, team.losses, team.gf, team.ga, team.gd, team.points];
            if (team.name === userTeam) fill(150, 255, 150);
            else fill(220);
            teamData.forEach((data, j) => {
                textAlign(j === 1 ? LEFT : CENTER, CENTER);
                text(data, j === 1 ? currentX : currentX + colWidths[j] / 2, rowY);
                currentX += colWidths[j];
            });
        });
    });
}

// [수정됨] 선수단 정보 화면 그리기 함수
function drawRosterScreen() {
    if (!userTeam) return;
    textAlign(CENTER, TOP);
    fill(255);
    textSize(28);
    text(`🧑‍🤝‍🧑 ${countryNameMap[userTeam]} 선수단 정보 🧑‍🤝‍🧑`, width / 2, 30);

    const teamPlayers = players.filter(p => p.team === userTeam);
    const startY = 80;
    const rowHeight = 22;
    // 모든 능력치를 표시하기 위해 컬럼 위치와 개수 재조정
    const colXs = [40, 170, 250, 310, 370, 430, 490, 550, 610, 690, 820]; 

    fill(180);
    textSize(14);
    textAlign(LEFT, CENTER);
    text("이름", colXs[0], startY);
    text("포지션", colXs[1], startY);
    text("능력치", colXs[2], startY);
    text("속도", colXs[3], startY);
    text("슛", colXs[4], startY);
    text("패스", colXs[5], startY);
    text("드리블", colXs[6], startY);
    text("수비", colXs[7], startY);
    text("피지컬", colXs[8], startY);
    text("GK", colXs[9], startY);
    text("상태", colXs[10], startY);
    stroke(100);
    line(colXs[0] - 10, startY + rowHeight / 2 + 2, width - 50, startY + rowHeight / 2 + 2);
    noStroke();

    for (let i = 0; i < teamPlayers.length; i++) {
        const player = teamPlayers[i];
        const y = startY + (i + 1) * rowHeight;

        fill(255);
        textSize(14);
        textAlign(LEFT, CENTER);
        text(player.koreanName, colXs[0], y);
        text(player.position, colXs[1], y);
        text(player.stat, colXs[2], y);
        text(player.speed !== null ? player.speed : '-', colXs[3], y);
        text(player.shot !== null ? player.shot : '-', colXs[4], y);
        text(player.pass !== null ? player.pass : '-', colXs[5], y);
        text(player.dribble !== null ? player.dribble : '-', colXs[6], y);
        text(player.defense !== null ? player.defense : '-', colXs[7], y);
        text(player.physical !== null ? player.physical : '-', colXs[8], y);
        text(player.gk !== null ? player.gk : '-', colXs[9], y);

        let statusText = '정상';
        let statusColor = color(144, 238, 144);

        if (player.injury > 0) {
            statusText = `🚑 부상 (${player.injury}경기)`;
            statusColor = color(255, 100, 100);
        } else if (player.suspension > 0) {
            statusText = `🟥 징계 (${player.suspension}경기)`;
            statusColor = color(255, 180, 100);
        } else if (player.yellowCards > 0) {
            statusText = `🟨 경고 (${player.yellowCards}장)`;
            statusColor = color(255, 255, 150);
        }
        fill(statusColor);
        text(statusText, colXs[10], y);
    }

    buttons.ROSTER_VIEW.forEach(btn => drawButton(btn));
}


function drawSquadManagementScreen() {
    textAlign(CENTER, TOP);
    fill(255);
    textSize(28);
    text("📜 스쿼드 관리 📜", width / 2, 30);

    const lineupX = width / 4,
        subsX = width * 3 / 4;
    const startY = 80;

    fill(255);
    textSize(18);
    textAlign(CENTER, TOP);
    text(`선발 라인업 (${userSquad.lineup.length} / 11)`, lineupX, startY);
    stroke(150);
    line(lineupX - 150, startY + 25, lineupX + 150, startY + 25);
    noStroke();

    text("후보 선수", subsX, startY);
    stroke(150);
    line(subsX - 150, startY + 25, subsX + 150, startY + 25);
    noStroke();

    const drawPlayerBox = (player, x, y) => {
        const boxW = 220,
            boxH = 25;
        const finalY = y + squadScrollOffset;

        if (finalY < startY + 30 || finalY > height - 100) return;

        const isHover = mouseX > x - boxW / 2 && mouseX < x + boxW / 2 && mouseY > finalY && mouseY < finalY + boxH;
        fill(isHover ? color(80, 100, 120) : color(40, 60, 80));
        rectMode(CORNER);
        rect(x - boxW / 2, finalY, boxW, boxH, 4);
        fill(220);
        textSize(12);
        textAlign(LEFT, CENTER);
        text(`${player.koreanName} (${player.position}, ${player.stat})`, x - boxW / 2 + 10, finalY + boxH / 2);
    };

    userSquad.lineup.forEach((p, i) => drawPlayerBox(p, lineupX, startY + 40 + i * 30));
    userSquad.substitutes.forEach((p, i) => drawPlayerBox(p, subsX, startY + 40 + i * 30));

    const {
        isValid,
        message
    } = validateSquad(userSquad);
    if (!isValid) {
        fill(255, 100, 100);
        textSize(16);
        textAlign(CENTER, CENTER);
        text(message, width / 2, height - 90);
    }

    buttons.SQUAD_MANAGEMENT.forEach(btn => drawButton(btn));
}


function handleSquadManagementClick() {
    const lineupX = width / 4,
        subsX = width * 3 / 4;
    const startY = 80,
        boxW = 220,
        boxH = 25,
        gapY = 30;

    for (let i = userSquad.lineup.length - 1; i >= 0; i--) {
        const p = userSquad.lineup[i];
        const y = startY + 40 + i * gapY + squadScrollOffset;
        if (mouseX > lineupX - boxW / 2 && mouseX < lineupX + boxW / 2 && mouseY > y && mouseY < y + boxH) {
            userSquad.substitutes.push(p);
            userSquad.lineup.splice(i, 1);
            userSquad.substitutes.sort((a, b) => b.stat - a.stat);
            return;
        }
    }

    for (let i = userSquad.substitutes.length - 1; i >= 0; i--) {
        const p = userSquad.substitutes[i];
        const y = startY + 40 + i * gapY + squadScrollOffset;
        if (mouseX > subsX - boxW / 2 && mouseX < subsX + boxW / 2 && mouseY > y && mouseY < y + boxH) {
            userSquad.lineup.push(p);
            userSquad.substitutes.splice(i, 1);
            userSquad.lineup.sort((a, b) => b.stat - a.stat);
            return;
        }
    }
}


function validateSquad(squad) {
    if (squad.lineup.length !== 11) {
        return {
            isValid: false,
            message: "선발 라인업은 반드시 11명이어야 합니다."
        };
    }
    const gkCount = squad.lineup.filter(p => p.position === 'GK').length;
    if (gkCount !== 1) {
        return {
            isValid: false,
            message: "골키퍼(GK)는 반드시 1명이어야 합니다."
        };
    }
    return {
        isValid: true,
        message: "스쿼드가 유효합니다."
    };
}

// =================================================
//   핵심 게임 로직
// =================================================
const actualGroups2018 = {
    A: ["Russia", "Saudi Arabia", "Egypt", "Uruguay"],
    B: ["Portugal", "Spain", "Morocco", "Iran"],
    C: ["France", "Australia", "Peru", "Denmark"],
    D: ["Argentina", "Iceland", "Croatia", "Nigeria"],
    E: ["Brazil", "Switzerland", "Costa Rica", "Serbia"],
    F: ["Germany", "Mexico", "Sweden", "South Korea"],
    G: ["Belgium", "Panama", "Tunisia", "England"],
    H: ["Poland", "Senegal", "Colombia", "Japan"]
};

function setupGroups(type) {
    let allNames = Object.keys(countryNameMap);
    if (type === 'random') allNames = shuffle(allNames);
    groups = [];
    for (let i = 0; i < 8; i++) {
        const gName = String.fromCharCode(65 + i);
        const tNames = type === 'actual' ? actualGroups2018[gName] : allNames.slice(i * 4, (i + 1) * 4);
        const teams = tNames.map(name => ({
            name,
            points: 0,
            wins: 0,
            draws: 0,
            losses: 0,
            gf: 0,
            ga: 0,
            gd: 0
        }));
        const matches = [{
            teams: [teams[0], teams[1]]
        }, {
            teams: [teams[2], teams[3]]
        }, {
            teams: [teams[0], teams[2]]
        }, {
            teams: [teams[1], teams[3]]
        }, {
            teams: [teams[0], teams[3]]
        }, {
            teams: [teams[1], teams[2]]
        }];
        matches.forEach(m => {
            m.played = false;
            m.scoreA = 0;
            m.scoreB = 0;
        });
        groups.push({
            name: gName,
            teams,
            matches
        });
    }
    currentStage = 'GROUP_STAGE';
    gameState = 'GROUP_STAGE';
}

function advanceTournament() {
    if (gameState === 'AWARDS_CEREMONY' || gameState === 'PRE_MATCH_BRIEFING') return;

    players.forEach(p => {
        if (p.injury > 0) p.injury--;
        if (p.suspension > 0) p.suspension--;
    });

    let nextMatchInfo = findNextMatch();

    if (nextMatchInfo) {
        const {
            match
        } = nextMatchInfo;
        const teamsInMatch = match.teams;

        if (teamsInMatch.some(t => t.name === userTeam)) {
            currentMatch = teamsInMatch;
            updatePlayerConditions(currentMatch.map(t => t.name));
            userSquad = autoSelectSquad(userTeam);
            gameState = 'PRE_MATCH_BRIEFING';
        } else {
            playAIMatch(match);
            gameState = currentStage === 'GROUP_STAGE' ? 'GROUP_STAGE' : 'KNOCKOUT';
        }
    } else {
        setupNextStage();
    }
}


function findNextMatch() {
    if (currentStage === 'GROUP_STAGE') {
        for (let matchIndex = 0; matchIndex < 6; matchIndex++) {
            for (const group of groups) {
                const match = group.matches[matchIndex];
                if (!match.played) {
                    return {
                        match: match
                    };
                }
            }
        }
    } else if (tournamentBracket[currentStage]) {
        const match = tournamentBracket[currentStage].find(m => !m.winner);
        if (match) {
            return {
                match: match
            };
        }
    }
    return null;
}

function setupNextStage() {
    const stageTransitions = {
        'GROUP_STAGE': 'round_of_16',
        'round_of_16': 'quarter_finals',
        'quarter_finals': 'semi_finals',
        'semi_finals': 'third_place_playoff',
        'third_place_playoff': 'final'
    };
    const nextStage = stageTransitions[currentStage];

    if (nextStage) {
        if (currentStage === 'GROUP_STAGE') {
            groups.forEach(g => g.teams.sort((a, b) => b.points - a.points || b.gd - a.gd || b.gf - a.gf));
            generateRoundOf16Bracket();
        } else if (currentStage === 'semi_finals') {
            const semis = tournamentBracket.semi_finals;
            const winners = semis.map(m => m.winner);
            const losers = semis.map(m => m.teams.find(t => t.name !== m.winner.name));
            tournamentBracket.final = [{
                teams: winners,
                winner: null,
                scoreA: 0,
                scoreB: 0
            }];
            tournamentBracket.third_place_playoff = [{
                teams: losers,
                winner: null,
                scoreA: 0,
                scoreB: 0
            }];
        } else if (currentStage !== 'third_place_playoff' && currentStage !== 'final') {
            generateNextRoundBracket(currentStage, nextStage);
        }
        currentStage = nextStage;
        gameState = 'KNOCKOUT';
    } else if (currentStage === 'final') {
        calculateAwards();
        gameState = 'AWARDS_CEREMONY';
    }
}


function playAIMatch(match) {
    const teams = match.teams;
    updatePlayerConditions(teams.map(t => t.name));
    const {
        squadA,
        squadB,
        scoreA,
        scoreB,
        yellows,
        reds
    } = simulateMatch(teams[0].name, teams[1].name);

    for (let i = 0; i < scoreA; i++) {
        const scorers = squadA.lineup.filter(p => p.position !== 'GK');
        if (scorers.length > 0) {
            const scorer = scorers[Math.floor(Math.random() * scorers.length)];
            scorer.goals++;
            if (Math.random() < 0.7) {
                const assisters = squadA.lineup.filter(p => p.name !== scorer.name);
                if (assisters.length > 0) assisters[Math.floor(Math.random() * assisters.length)].assists++;
            }
        }
    }
    for (let i = 0; i < scoreB; i++) {
        const scorers = squadB.lineup.filter(p => p.position !== 'GK');
        if (scorers.length > 0) {
            const scorer = scorers[Math.floor(Math.random() * scorers.length)];
            scorer.goals++;
            if (Math.random() < 0.7) {
                const assisters = squadB.lineup.filter(p => p.name !== scorer.name);
                if (assisters.length > 0) assisters[Math.floor(Math.random() * assisters.length)].assists++;
            }
        }
    }

    let finalScoreA = scoreA,
        finalScoreB = scoreB;
    if (currentStage === 'GROUP_STAGE') {
        updateGroupStats(teams[0], teams[1], finalScoreA, finalScoreB);
        match.played = true;
        match.scoreA = finalScoreA;
        match.scoreB = finalScoreB;
    } else {
        if (finalScoreA === finalScoreB) {
            if (getTeamStrength(teams[0].name) > getTeamStrength(teams[1].name)) finalScoreA++;
            else finalScoreB++;
        }
        match.scoreA = finalScoreA;
        match.scoreB = finalScoreB;
        match.winner = finalScoreA > finalScoreB ? teams[0] : teams[1];
    }
    applyPostMatchEffects(teams.map(t => t.name), {
        yellows,
        reds
    });
}


function returnToTournamentView() {
    gameState = currentStage === 'GROUP_STAGE' ? 'GROUP_STAGE' : 'KNOCKOUT';
}

function calculateAwards() {
    const final = tournamentBracket.final[0];
    const thirdMatch = tournamentBracket.third_place_playoff[0];
    awards.first = final.winner;
    awards.second = final.teams.find(t => t.name !== final.winner.name);
    awards.third = thirdMatch.winner;
    const findTop = (stat, tiebreaker = 'goals') => {
        const sorted = [...players].sort((a, b) => b[stat] - a[stat] || b[tiebreaker] - a[tiebreaker]);
        const topValue = sorted[0] ?.[stat];
        if (!topValue) return [];
        return sorted.filter(p => p[stat] === topValue).map(p => ({
            name: p.koreanName,
            team: p.team,
            stat: `${p[stat]}${stat==='goals'?'골':'도움'}`
        }));
    };
    const findSoloWinner = (stat, tiebreaker = 'goals') => {
        const sorted = [...players].sort((a, b) => b[stat] - a[stat] || b[tiebreaker] - a[tiebreaker]);
        if (!sorted[0] || sorted[0][stat] === 0) return [];
        return sorted.slice(0, 1).map(p => ({
            name: p.koreanName,
            team: p.team,
            stat: `${p[stat]}${stat==='assists'?'도움':'골'}`
        }));
    };
    awards.goldenBoot = findTop('goals', 'assists');
    awards.assistKing = findSoloWinner('assists', 'goals');
    const young = players.filter(p => p.age <= 23).sort((a, b) => (b.goals + b.assists) - (a.goals + a.assists));
    if (young.length > 0 && young[0].goals + young[0].assists > 0) awards.youngPlayer = young.slice(0, 1).map(p => ({
        name: p.koreanName,
        team: p.team,
        stat: `${p.goals}골 ${p.assists}도움`
    }));
    else awards.youngPlayer = [];
}

function generateRoundOf16Bracket() {
    const getTeam = (groupLetter, rank) => groups.find(g => g.name === groupLetter).teams[rank - 1];
    tournamentBracket.round_of_16 = [{
        teams: [getTeam('A', 1), getTeam('B', 2)]
    }, {
        teams: [getTeam('C', 1), getTeam('D', 2)]
    }, {
        teams: [getTeam('E', 1), getTeam('F', 2)]
    }, {
        teams: [getTeam('G', 1), getTeam('H', 2)]
    }, {
        teams: [getTeam('B', 1), getTeam('A', 2)]
    }, {
        teams: [getTeam('D', 1), getTeam('C', 2)]
    }, {
        teams: [getTeam('F', 1), getTeam('E', 2)]
    }, {
        teams: [getTeam('H', 1), getTeam('G', 2)]
    }].map(m => ({ ...m,
        winner: null,
        scoreA: 0,
        scoreB: 0
    }));
}

function generateNextRoundBracket(prevStageKey, nextStageKey) {
    const winners = tournamentBracket[prevStageKey].map(m => m.winner);
    tournamentBracket[nextStageKey] = [];
    for (let i = 0; i < winners.length; i += 2) {
        tournamentBracket[nextStageKey].push({
            teams: [winners[i], winners[i + 1]],
            winner: null,
            scoreA: 0,
            scoreB: 0
        });
    }
}

function updateGroupStats(teamA, teamB, scoreA, scoreB) {
    teamA.gf += scoreA;
    teamA.ga += scoreB;
    teamB.gf += scoreB;
    teamB.ga += scoreA;
    teamA.gd = teamA.gf - teamA.ga;
    teamB.gd = teamB.gf - teamB.ga;
    if (scoreA > scoreB) {
        teamA.wins++;
        teamB.losses++;
        teamA.points += 3;
    } else if (scoreA < scoreB) {
        teamB.wins++;
        teamA.losses++;
        teamB.points += 3;
    } else {
        teamA.draws++;
        teamB.draws++;
        teamA.points += 1;
    }
}

// =================================================
//   상세 경기 시뮬레이션 및 선수 관리
// =================================================
function startMatchSimulation() {
    gameMinute = 0;
    frameCounterForSim = 0;
    matchLog = [`[0'] ${countryNameMap[currentMatch[0].name]}과(와) ${countryNameMap[currentMatch[1].name]}의 경기가 시작됩니다!`];
    currentMatchResult = {
        scoreA: 0,
        scoreB: 0,
        scorers: [],
        injuries: [],
        yellows: [],
        reds: [],
        mom: null
    };
    logScrollOffset = 0;
    gameState = 'MATCH_SIMULATION';
}

function simulateMatch(teamAName, teamBName) {
    const squadA = getSquadByFormation(teamAName),
        squadB = getSquadByFormation(teamBName);
    const strengthA = getTeamStrength(teamAName),
        strengthB = getTeamStrength(teamBName);
    const powerA = strengthA * (0.8 + Math.random() * 0.4),
        powerB = strengthB * (0.8 + Math.random() * 0.4);
    let yellows = [],
        reds = [];
    const allPlayersInMatch = [...squadA.lineup, ...squadB.lineup];
    allPlayersInMatch.forEach(p => {
        if (Math.random() < 0.1) {
            yellows.push(p);
            if (yellows.filter(pl => pl.name === p.name).length === 2) {
                reds.push(p);
            }
        }
        if (Math.random() < 0.01) {
            reds.push(p);
        }
    });
    return {
        squadA,
        squadB,
        scoreA: Math.floor(Math.pow(powerA / 40, 2)),
        scoreB: Math.floor(Math.pow(powerB / 40, 2)),
        yellows,
        reds
    };
}

function updateMatchSimulation() {
    if (gameMinute >= 90) {
        if (gameState === 'MATCH_SIMULATION') endMatchSimulation();
        return;
    }
    frameCounterForSim++;
    if (frameCounterForSim >= framesPerGameMinute) {
        frameCounterForSim = 0;
        gameMinute++;
        if (Math.random() < 0.35) {
            const [teamA, teamB] = currentMatch;
            const strA = getTeamStrength(teamA.name),
                strB = getTeamStrength(teamB.name);
            const possTeamName = (Math.random() * (strA + strB) < strA) ? teamA.name : teamB.name;
            const eventLog = generateMatchEvent(possTeamName);
            if (eventLog) {
                matchLog.push(`[${gameMinute}'] ${eventLog}`);
                logScrollOffset = -max(0, (matchLog.length * 25) - (height - 150));
            }
        }
    }
}

function generateMatchEvent(teamName) {
    const squad = getSquadByFormation(teamName);
    if (squad.lineup.length === 0) return null;
    const opponent = currentMatch.find(t => t.name !== teamName);
    const {
        lineup: opponentLineup
    } = getSquadByFormation(opponent.name);
    const eventType = Math.random();

    if (eventType < 0.25) {
        const shooter = squad.lineup.filter(p => p.position !== 'GK').sort((a, b) => b.shot - a.shot)[0];
        const oppGK = opponentLineup.find(p => p.position === 'GK');
        if (shooter) {
            if (Math.random() * 100 < shooter.shot * (shooter.condition / 100) - (oppGK?.gk * (oppGK.condition / 100) || 60) + 25) {
                const isTeamA = currentMatch[0].name === teamName;
                if (isTeamA) currentMatchResult.scoreA++;
                else currentMatchResult.scoreB++;
                shooter.goals++;
                if (Math.random() < 0.8) {
                    const assisters = squad.lineup.filter(p => p.position !== 'GK' && p.name !== shooter.name).sort((a, b) => b.pass - a.pass);
                    if (assisters.length > 0) assisters[0].assists++;
                }
                currentMatchResult.scorers.push({
                    player: shooter,
                    minute: gameMinute,
                    teamName
                });
                return `⚽ GOAL!!! ${shooter.koreanName}의 득점!`;
            } else {
                return `${shooter.koreanName}의 슈팅! ${oppGK?.koreanName||'골키퍼'} 선방!`;
            }
        }
    } else if (eventType < 0.5) {
        const fouler = squad.lineup.sort((a, b) => b.defense - a.defense)[0];
        const cardProb = Math.random();
        if (cardProb < 0.03 && !currentMatchResult.reds.some(p => p.name === fouler.name)) {
            currentMatchResult.reds.push(fouler);
            return `🟥 레드카드! ${fouler.koreanName}이(가) 거친 태클로 퇴장당합니다!`;
        } else if (cardProb < 0.2) {
            if (!currentMatchResult.reds.some(p => p.name === fouler.name)) {
                currentMatchResult.yellows.push(fouler);
                if (currentMatchResult.yellows.filter(p => p.name === fouler.name).length === 2) {
                    currentMatchResult.reds.push(fouler);
                    return `🟥 경고 누적 퇴장! ${fouler.koreanName}이(가) 두 번째 옐로카드를 받고 퇴장합니다!`;
                }
                return `🟨 옐로카드! ${fouler.koreanName}에게 경고가 주어집니다.`;
            }
        } else {
            return `${fouler.koreanName}의 깊은 태클! 파울이 선언됩니다.`;
        }
    } else if (eventType < 0.75) {
        const dribbler = squad.lineup.filter(p => ['FW', 'MF'].includes(p.position)).sort((a, b) => b.dribble - a.dribble)[0];
        const oppDef = opponentLineup.filter(p => p.position === 'DF').sort((a, b) => b.defense - a.defense)[0];
        if (dribbler && oppDef && Math.random() * 100 < dribbler.dribble * (dribbler.condition / 100) - oppDef.defense * (oppDef.condition / 100) + 30) return `${dribbler.koreanName}의 환상적인 돌파!`;
        else if (oppDef) return `${oppDef.koreanName}의 깔끔한 수비!`;
    }
    return `${countryNameMap[teamName]}이(가) 공격을 전개합니다.`;
}


function endMatchSimulation() {
    const [teamA, teamB] = currentMatch;
    let matchInfo = findNextMatch();

    if (matchInfo) {
        const {
            match
        } = matchInfo;
        if (currentStage === 'GROUP_STAGE') {
            updateGroupStats(teamA, teamB, currentMatchResult.scoreA, currentMatchResult.scoreB);
            match.played = true;
            match.scoreA = currentMatchResult.scoreA;
            match.scoreB = currentMatchResult.scoreB;
        } else {
            if (currentMatchResult.scoreA === currentMatchResult.scoreB) {
                if (getTeamStrength(teamA.name) >= getTeamStrength(teamB.name)) currentMatchResult.scoreA++;
                else currentMatchResult.scoreB++;
            }
            match.scoreA = currentMatchResult.scoreA;
            match.scoreB = currentMatchResult.scoreB;
            match.winner = currentMatchResult.scoreA > currentMatchResult.scoreB ? teamA : teamB;
        }
    }

    const {
        lineup: lineupA
    } = getSquadByFormation(teamA.name), {
        lineup: lineupB
    } = getSquadByFormation(teamB.name);
    calculatePlayerRatings(lineupA, currentMatchResult.scoreA, currentMatchResult.scoreB);
    calculatePlayerRatings(lineupB, currentMatchResult.scoreB, currentMatchResult.scoreA);
    currentMatchResult.mom = [...lineupA, ...lineupB].sort((a, b) => b.rating - a.rating)[0];
    applyPostMatchEffects([teamA.name, teamB.name], currentMatchResult);

    gameState = 'MATCH_RESULT';
}


function getSquadByFormation(teamName) {
    if (teamName === userTeam) {
        return userSquad;
    }
    return autoSelectSquad(teamName);
}

function autoSelectSquad(teamName) {
    const available = players.filter(p => p.team === teamName && p.injury === 0 && p.suspension === 0);
    const pos = {
        GK: [],
        DF: [],
        MF: [],
        FW: []
    };
    available.forEach(p => pos[p.position]?.push(p));
    Object.keys(pos).forEach(k => pos[k].sort((a, b) => b.stat - a.stat));

    const pickBest = (formation) => {
        let lineup = [];
        const pCopy = JSON.parse(JSON.stringify(pos));
        for (const [posKey, num] of Object.entries(formation)) {
            if (pCopy[posKey].length < num) return null;
            lineup.push(...pCopy[posKey].splice(0, num));
        }
        return lineup;
    };

    let lineup = pickBest({
        GK: 1,
        DF: 4,
        MF: 4,
        FW: 2
    }) || pickBest({
        GK: 1,
        DF: 4,
        MF: 3,
        FW: 3
    }) || pickBest({
        GK: 1,
        DF: 3,
        MF: 5,
        FW: 2
    }) || pickBest({
        GK: 1,
        DF: 5,
        MF: 4,
        FW: 1
    });
    if (!lineup) {
        lineup = available.sort((a, b) => b.stat - a.stat).slice(0, 11);
    }

    const substitutes = available.filter(p => !lineup.some(l => l.name === p.name)).sort((a, b) => b.stat - a.stat);
    return {
        lineup,
        substitutes
    };
}


function applyPostMatchEffects(teamNames, matchResult) {
    teamNames.forEach(name => {
        players.filter(p => p.team === name).forEach(p => {
            if (p.injury === 0 && Math.random() < 0.05) {
                p.injury = Math.floor(Math.random() * 2) + 1;
            }
        });
    });
    if (matchResult) {
        matchResult.reds?.forEach(redPlayer => {
            const p = players.find(pl => pl.name === redPlayer.name);
            if (p) p.suspension = 1;
        });
        matchResult.yellows?.forEach(yellowPlayer => {
            const p = players.find(pl => pl.name === yellowPlayer.name);
            if (p && p.suspension === 0) {
                p.yellowCards++;
                if (p.yellowCards >= 2) {
                    p.suspension = 1;
                    p.yellowCards = 0;
                }
            }
        });
    }
}

function getTeamStrength(teamName) {
    const {
        lineup
    } = getSquadByFormation(teamName);
    if (lineup.length < 11) return 60 - ((11 - lineup.length) * 5);
    const totalStrength = lineup.reduce((sum, p) => sum + (p.stat * (p.condition / 100)), 0);
    return totalStrength / lineup.length;
}

function calculatePlayerRatings(lineup, gf, ga) {
    lineup.forEach(p => {
        let r = 6.5;
        if (gf > ga) r += 0.5;
        else if (gf < ga) r -= 0.3;
        r += currentMatchResult.scorers.filter(s => s.player.name === p.name).length * 1.5;
        r -= currentMatchResult.yellows.filter(s => s.name === p.name).length * 0.5;
        r -= currentMatchResult.reds.filter(s => s.name === p.name).length * 1.5;
        if ((p.position === 'DF' || p.position === 'GK') && ga === 0) r += 1.0;
        if (p.position === 'GK' && ga > 2) r -= 0.5;
        r += (p.condition - 90) * 0.02;
        p.rating = Math.max(4.0, Math.min(10.0, r));
    });
}

function populatePlayerStats() {
    players.forEach(p => {
        if (p.stat === undefined || p.stat === null) {
            if (p.position === 'GK') {
                p.stat = Math.floor((p.gk || 0) * 0.8 + (p.pass || 0) * 0.1 + (p.physical || 0) * 0.1);
            } else {
                p.stat = Math.floor((p.shot || 0) * 0.2 + (p.pass || 0) * 0.2 + (p.dribble || 0) * 0.15 + (p.defense || 0) * 0.15 + (p.physical || 0) * 0.15);
            }
        }
        p.goals = 0;
        p.assists = 0;
        p.rating = 0;
        p.injury = 0;
        p.condition = 0;
        p.yellowCards = 0;
        p.suspension = 0;
    });
}

function updatePlayerConditions(names) {
    const teams = names || players.map(p => p.team);
    players.forEach(p => {
        if (teams.includes(p.team)) p.condition = 75 + Math.floor(Math.random() * 26);
    });
}

function shuffle(array) {
    let i = array.length,
        r;
    while (i != 0) {
        r = Math.floor(Math.random() * i);
        i--;
        [array[i], array[r]] = [array[r], array[i]];
    }
    return array;
}

// =================================================
//   선수 상세 정보 및 클릭 처리
// =================================================

function drawPlayerDetailScreen() {
    if (!selectedPlayer) return;

    textAlign(CENTER, TOP);
    fill(255);
    textSize(32);
    text(`⭐ 선수 상세 정보 ⭐`, width / 2, 40);

    textSize(24);
    text(`${selectedPlayer.koreanName} (${selectedPlayer.name})`, width / 2, 100);

    textSize(18);
    fill(220);
    text(`소속팀: ${countryNameMap[selectedPlayer.team]} | 포지션: ${selectedPlayer.position} | 나이: ${selectedPlayer.age}`, width / 2, 140);

    const startY = 200;
    const lineHeight = 35;
    const statX = width / 2 - 150;
    const barX = width / 2 - 50;
    const barW = 200;

    const stats = [{
        label: '속도',
        value: selectedPlayer.speed
    }, {
        label: '슈팅',
        value: selectedPlayer.shot
    }, {
        label: '패스',
        value: selectedPlayer.pass
    }, {
        label: '드리블',
        value: selectedPlayer.dribble
    }, {
        label: '수비',
        value: selectedPlayer.defense
    }, {
        label: '피지컬',
        value: selectedPlayer.physical
    }];

    if (selectedPlayer.position === 'GK') {
        stats.push({
            label: '골키퍼',
            value: selectedPlayer.gk
        });
    }

    stats.forEach((stat, i) => {
        if (stat.value === null || stat.value === undefined) return;

        const y = startY + i * lineHeight;
        textAlign(RIGHT, CENTER);
        textSize(16);
        fill(255);
        text(stat.label, statX, y);

        fill(50, 80, 110);
        rect(barX, y - 10, barW, 20, 5);

        const statColor = color(100, 180, 255);
        fill(statColor);
        rect(barX, y - 10, (stat.value / 99) * barW, 20, 5);

        textAlign(LEFT, CENTER);
        fill(255);
        text(stat.value, barX + barW + 10, y);
    });


    buttons.PLAYER_DETAIL.forEach(btn => drawButton(btn));
}

// [수정됨] 선수 이름 클릭 처리 함수
function handlePlayerNameClick() {
    let clickAreas = [];

    const addPlayerClickArea = (squad, x, y, isLeft) => {
        const xOffset = isLeft ? -100 : -60;
        
        squad.lineup.forEach((p, i) => {
            clickAreas.push({ player: p, x: x + xOffset, y: y + i * 24, w: 160, h: 20 });
        });

        const subStartY = y + squad.lineup.length * 24 + 10;
        squad.substitutes.forEach((p, i) => {
            clickAreas.push({ player: p, x: x - 100, y: subStartY + 25 + i * 20, w: 200, h: 20 });
        });
    };

    if (gameState === 'PRE_MATCH_BRIEFING' || gameState === 'MATCH_RESULT') {
        const [teamA, teamB] = currentMatch;
        addPlayerClickArea(getSquadByFormation(teamA.name), width / 4, 110, true);
        addPlayerClickArea(getSquadByFormation(teamB.name), width * 3 / 4, 110, false);
    } else if (gameState === 'ROSTER_VIEW') {
        const teamPlayers = players.filter(p => p.team === userTeam);
        teamPlayers.forEach((p, i) => {
            clickAreas.push({ player: p, x: 50, y: 80 + (i + 1) * 22 - 10, w: 120, h: 20 });
        });
    }

    for (const area of clickAreas) {
        if (mouseX > area.x && mouseX < area.x + area.w && mouseY > area.y && mouseY < area.y + area.h) {
            selectedPlayer = area.player;
            previousGameState = gameState; // 상태 전환 직전에 현재 상태 저장
            gameState = 'PLAYER_DETAIL';
            return;
        }
    }
}
